function demo(input){
let host = 0; let guest = 0;
let win = 0; let loss = 0; let draw = 0;
for(i = 0; i < 3; i++){
host = Number((input[i])[0]);
guest = Number((input[i])[2]);
if(host > guest){
win++;
}
else if(host < guest){
loss++;
}
else if(host == guest){
draw++;
}
}
console.log(`Team won ${win} games.`);
console.log(`Team lost ${loss} games.`);
console.log(`Drawn games: ${draw}`);
}
